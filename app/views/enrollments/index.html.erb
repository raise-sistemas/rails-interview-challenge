<p style="color: green"><%= notice %></p>

<% content_for :title, "Enrollments" %>

<h1>Enrollments</h1>

<div id="enrollments">
  <% @enrollments.each do |enrollment| %>
    <% cache enrollment do %>
      <%= render enrollment %>
    <% end %>
    <p>
      <%= link_to "Show this enrollment", enrollment %>
    </p>
  <% end %>
</div>

<%= link_to "New enrollment", new_enrollment_path %>
